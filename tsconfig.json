{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "@fino-ngx-doc-scanner": [
        "libs/fino-ngx-doc-scanner/projects/ngx-document-scanner/src/public_api"
      ],
      "@fino/store": [
        "src/app/store/index.ts"
      ]
    }
  },
  "angularCompilerOptions": {
    "enableIvy": false
  }
}
